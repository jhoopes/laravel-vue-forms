{
  "name": "jhoopes/laravel-vue-forms",
  "description": "Laravel Vue Forms Package",
  "license": "MIT",
  "authors": [
    {
      "name": "John Hoopes"
    }
  ],
  "require": {
    "php": ">=7.2",
    "mews/purifier": "3.2.*",
    "neomerx/json-api": "^4.0",
    "laravel/framework": "5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ~6 || ~7"
  },
  "autoload": {
    "psr-4": {
      "jhoopes\\LaravelVueForms\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "jhoopes\\LaravelVueForms\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "jhoopes\\LaravelVueForms\\Providers\\LaravelVueFormsServiceProvider"
      ],
      "aliases": {
        "LaravelForms": "jhoopes\\LaravelVueForms\\Facades\\LaravelVueForms"
      }
    }
  }
}
