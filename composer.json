{
    "name": "jhoopes/laravel-vue-forms",
    "description": "Laravel Vue Forms Package",
    "license": "MIT",
    "authors": [
        {
            "name": "John Hoopes"
        }
    ],
    "require": {
        "php": "^8.0",
        "mews/purifier": "3.3.*",
        "neomerx/json-api": "^4.0",
        "laravel/framework": "~8",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "nunomaduro/larastan": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "jhoopes\\LaravelVueForms\\": "src/",
            "jhoopes\\LaravelVueForms\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jhoopes\\LaravelVueForms\\Test\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "jhoopes\\LaravelVueForms\\Providers\\LaravelVueFormsServiceProvider"
            ],
            "aliases": {
                "LaravelForms": "jhoopes\\LaravelVueForms\\Facades\\LaravelVueForms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
